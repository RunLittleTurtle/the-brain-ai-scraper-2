{
  "title": "The Brain AI Scraper",
  "description": "Intent inference workflow",
  "dependencies": ["."],
  "env": ".env",
  "config": {
    "tracing": {
      "type": "langsmith",
      "project": "brain_ai_scraper"
    }
  },
  "version": "v1",
  "paths": {
    "intent_inference.graph.app": {
      "dependencies": [
        "intent_inference.graph.subgraphs.intent_processing",
        "intent_inference.graph.subgraphs.feedback_processing",
        "intent_inference.graph.subgraphs.validation",
        "intent_inference.graph.nodes"
      ],
      "sample_inputs": [
        {
          "context": {
            "user_query": "Scrape product information from amazon.com for iPhones",
            "is_feedback": false
          }
        }
      ]
    }
  },
  "graphs": {
    "intent_inference": {
      "path": "intent_inference.graph.app:get_intent_graph",
      "description": "Parse user query into intent_spec"
    }
  }
}
